def talk(animal_type, name)
  if animal_type == "bird"
    puts "#{name} says chirp chirp!"
  elsif animal_type =="dog"
    puts "#{name} says bark!"
  elsif animal_type =="cat"
    puts "#{name} says meow!"
  elsif animal_type =="lion"
    puts "#{name} says Roar!"
  elsif animal_type =="cow"
    puts "#{name} says Moo!"
  elsif animal_type =="duck"
    puts "#{name} says squawk!"
  end
end

def move(animal_type,name, destination)
  if animal_type =="bird"
    puts "#{name} flies to #{destination}"
  elsif animal_type =="dog"
    puts "#{name} runs to #{destination}"
  elsif animal_type =="cat"
    puts "#{name} runs to #{destination}"
  elsif animal_type =="lion"
    puts "#{name} runs to #{destination}"
  elsif animal_type =="cow"
    puts "#{name} runs to #{destination}"
  elsif animal_type =="duck"
    puts "#{name} runs to #{destination}"
  end
end



talk("cow", "phili")
move("cow","phili","barn")